<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate
Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class catalog="qlhocphan" name="pojo.TaiKhoan" table="TaiKhoan">
		<id name="tenTaiKhoan" type="string">
			<column length="30" name="TenTaiKhoan" />
			<generator class="assigned" />
		</id>
		<property name="matKhau" type="string">
			<column length="20" name="MatKhau" />
		</property>
		<property name="loai" type="string">
			<column length="2" name="Loai" />
		</property>
	</class>
</hibernate-mapping>