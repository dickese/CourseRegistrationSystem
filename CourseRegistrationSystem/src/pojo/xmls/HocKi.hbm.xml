<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate
Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class catalog="qlhocphan" name="pojo.HocKi" table="HocKi">

		<id name="maHK" column="MaHK" type="integer">
			<generator class="identity" />
		</id>

		<property name="tenHocKi" type="string">
			<column name="TenHocKi" length="3" />
		</property>

		<property name="namHoc" type="int">
			<column name="NamHoc" />
		</property>

		<property name="ngayBatDau" type="date">
			<column length="10" name="NgayBatDau" />
		</property>

		<property name="ngayKetThuc" type="date">
			<column length="10" name="NgayKetThuc" />
		</property>


	</class>
</hibernate-mapping>